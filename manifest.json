{
  "manifest_version": 2,

  "name": "Chromekey",
  "description": "vi key mapping for GoogleChrome",
  "version": "0.1",

  "permissions": [
    "tabs",
    "bookmarks"
  ],
  
  "background": {
    "persistent": true,
    "page": "background/background.html"
  },

  "content_scripts": [{
    "matches": [ "<all_urls>" ],
    "css": [
      "css/main.css"
    ],
    "js": [
      "lib/jquery-2.0.1.min.js",
      "js/key_identifiers.js",
      "js/key_event.js",
      "js/key_sequence.js",
      "js/viewport.js",
      "js/command_box.js",
      "js/mode/hint_element.js",
      "js/mode/hint_element_collection.js",
      "js/mode/normal.js",
      "js/mode/visual.js",
      "js/mode/command.js",
      "js/mode/bookmark.js",
      "js/mode/hint.js",
      "js/command.js",
      "js/keymap.js",
      "js/mode.js",
      "js/main.js"
    ]
  }]
}
